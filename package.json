{
  "name": "sport-garant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@alfalab/core-components": "47.24.1",
    "@reduxjs/toolkit": "^2.3.0",
    "@types/react-redux": "^7.1.34",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "normalize.css": "^8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.3",
    "arui-presets-lint": "8.0.0",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "vite": "^5.4.10"
  },
  "eslintConfig": {
    "extends": [
      "arui-presets-lint/eslint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "arui-presets-lint/prettier",
  "commitlint": {
    "extends": "./node_modules/arui-presets-lint/commitlint",
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "header-max-length": [
        2,
        "always",
        150
      ]
    }
  }
